        "import numpy as np\n",
        "from queue import Queue\n",
        "graph={0:[1,3],1:[0,2,3],2:[1,4,5],3:[0,1,4],4:[2,3,5],5:[2,4]}\n",
        "print(\"the adjacenc list representing the graph is:\")\n",
        "print(graph)"
